
<section class="contact-area" id="contact">
        <div class="contact-form-area section-padding gray-bg">
            <div class="container">
                <div class="row">
                    <div class="col-md-12 col-lg-12 col-sm-12 col-xs-12">
                        <div class="area-title text-center">
                          <div class="col-md-4 col-lg-4 col-sm-4 col-xs-4" >
                            <div id="success" class="alert alert-success" {{#if noMessages}}hidden{{/if}}>
                        {{successMsg}}
                             </div>
                        </div>
                            <h1>DASHBOARD</h1>
                            {{# each orders}}
                            <p  class="btn btn-success">Name:{{ this.name }}</p>
                            <p  class="btn btn-success"><strong>Address:{{ this.address }}</strong></p>
                            <p  class="btn btn-success"><strong>Payment Id:  {{ this.paymentId }}</strong>  </p>
                        </div>
                    </div>
                </div>
                <div class="row">
              
              <div class="col-md-10 col-lg-10 col-sm-12 col-xs-12">
                <p class="btn btn-success">Total Price:<strong>${{ this.cart.totalPrice }}</strong> </p>
                <div class="row">
                
                </div>
                {{# each this.items}}
                    <table class="table">
                        <thead class="thead-dark">
                          <tr>
                            <th scope="col">Title</th>
                            <th scope="col">Quantity</th>
                            <th scope="col">Price</th>
                          </tr>
                        </thead>
                        <tbody>
                          <tr>
                            <td>{{ this.item.title}}</td>
                            <td>{{this.qty}}</td>
                            <td>${{this.price}}</td>
                          </tr>
                        </tbody>
                    </table>
                    <hr> <hr>
                    {{/each}}
                  
                  
                  <hr>
              
             
 {{/each}}

              
              </div>
             </div>
              </div>
</section>